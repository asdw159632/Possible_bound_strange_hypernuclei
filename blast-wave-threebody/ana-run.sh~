#!/bin/bash

source /storage/fdunphome/chengyilin/.conenv.sh

inDir=/storage/fdunphome/chengyilin/coalescence/analysis-dynamicalCoal

outDir=$inDir/results
mkdir -p $outDir

export EXEH=$inDir/bin/analysis
export PAR=$inDir
export outFile=$outDir/result-$1
export LogFile=$outDir/log-$1.log

$EXEH $PAR $outFile > $LogFile

exit
